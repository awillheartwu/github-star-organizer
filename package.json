{
  "name": "github-star-organizer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "lint": "vue-tsc --noEmit",
    "commit": "git-cz",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.59.8",
    "@tanstack/vue-query-devtools": "^5.59.8",
    "axios": "^1.7.7",
    "naive-ui": "^2.39.0",
    "pinia": "^2.1.7",
    "vue": "^3.5.17",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "3.4.3",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vue-tsc": "^2.2.12",
    "prettier": "^3.6.2",
    "prettier-eslint": "^16.1.2",
    "standard-version": "^9.5.0",
    "commitizen": "^4.3.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "cz-conventional-changelog": "^3.3.0"
  },
  "lint-staged": {
    "{src,test}/**/*.{ts,js,json}": [
      "prettier --write '{src,test}/**/*.ts'",
      "eslint --fix",
      "stop-only --file"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
