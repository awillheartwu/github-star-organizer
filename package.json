{
  "name": "github-star-organizer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "lint": "pnpm run lint:eslint && pnpm run typecheck",
    "lint:eslint": "eslint --ext .ts,.tsx,.vue src *.cjs *.js *.ts",
    "commit": "git-cz",
    "preview": "vite preview",
    "test": "vitest run",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:html": "vitest run --coverage && open coverage/index.html",
    "lint:fix": "eslint --fix --ext .ts,.tsx,.vue src *.cjs *.js *.ts",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.59.8",
    "@tanstack/vue-query-devtools": "^5.59.8",
    "axios": "^1.7.7",
    "naive-ui": "^2.39.0",
    "pinia": "^2.1.7",
    "vue": "^3.5.17",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@types/testing-library__vue": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-eslint": "^16.1.2",
    "standard-version": "^9.5.0",
    "tailwindcss": "3.4.3",
    "typescript": "~5.6.3",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.2.12"
  },
  "lint-staged": {
    "{src,test}/**/*.{ts,js,json}": [
      "prettier --write '{src,test}/**/*.ts'",
      "eslint --fix",
      "stop-only --file"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
